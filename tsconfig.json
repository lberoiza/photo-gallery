{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/models": [
        "src/models/*"
      ],
      "@/data": [
        "src/data/*"
      ]
    }
  }
}